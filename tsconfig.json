{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "lib": [ "esnext", "dom" ],
      "esModuleInterop": true,
      "jsx": "react",
      "pretty": true,
      "strict": true,
      "skipLibCheck": true,
      "declaration": true,
      "removeComments": true,
      "emitDeclarationOnly": false,
      "outDir": "dist",
      "resolveJsonModule": true,
      "noImplicitThis": false,
      "baseUrl": "./src"
    },
    "include": ["./src"],
    "exclude": ["./node_modules/**/*"]
  }
  